<div class = "container">
<div class = "row title">
  <div class = "col-md-6 col-md-offset-3">Private Wikis</div>
</div>

<% if !current_user.present? || current_user.role == 'standard' %>
<div class = "row title"> NO ACCESS TO PRIVATE WIKIS </div>
<% else %>
  <div id = "post-buttons-center" class = "row col-md-6 col-md-offset-3"> <%= link_to "Create a new wiki", new_wiki_path %></div>

    <%= render :partial => 'wikilist' %>

  </div>
<% end %>