<div class = "container">
<div class = "row title">
  <div class = "col-md-6 col-md-offset-3">Public Wikis</div>
</div>

<div id = "post-buttons-center" class = "row col-md-6 col-md-offset-3"> <%= link_to "Create a new wiki", new_wiki_path %></div>

<% @wikis.each do |wiki| %>
  <div class="row"> 
    <div class = "wiki-link">
      <div class = "col-md-4 col-md-offset-3"><%= link_to wiki.title, wiki %></div>
      <div id = "edit-delete" class="col-md-2"><%= link_to "Edit", edit_wiki_path(wiki) %> | <%= link_to "Delete", wiki, method: :delete, data: {confirm: 'Are you sure you want to delete this wiki?' } %></div>
    </div>
  </div>
<% end %>
</div>