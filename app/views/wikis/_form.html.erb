<% if current_user.role == 'admin' || current_user.role == 'premium' %>
    <div class="form-group">
      <%= f.label :private, class: 'checkbox' do %>
      <%= f.check_box :private %> Private wiki


      <div>
      <% if @wiki.private? %>
        <%= f.label :collaborators %>
        <div><%= select_tag "collaborators", options_from_collection_for_select(User.all, 'id', 'name',@wiki.users.map{ |j| j.id }), :multiple => true %></div>
        <% end %>
        
      </div>
    <% end %>
  </div>
<% end %>